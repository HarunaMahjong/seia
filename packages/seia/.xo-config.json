{
	"prettier": true,
	"rules": {
		"import/order": [
			"warn",
			{
				"newlines-between": "always"
			}
		],
		"@typescript-eslint/consistent-type-definitions": "off",
		"@typescript-eslint/array-type": [
			"warn",
			{
				"default": "array-simple"
			}
		],
		"no-bitwise": "off",
		"import/no-unassigned-import": "off",
		"no-confusing-void-expression": "off",
		"no-return-await": "off"
	}
}
